{
  "name": "@libertymp/orpc",
  "version": "0.0.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "The type-safe Node.js and browser RPC library built on top of socket.io.",
  "repository": {
    "type": "git",
    "url": "https://github.com/liberty-mp/orpc"
  },
  "keywords": [
    "orpc",
    "rpc",
    "socket.io",
    "type-safe",
    "typescript",
    "AOC",
    "governance",
    "load-balancing"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "type-check": "tsc -p src --noEmit"
  },
  "main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
  "devDependencies": {
    "prettier": "^2.8.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "nanoid": "^4.0.0",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4"
  }
}
